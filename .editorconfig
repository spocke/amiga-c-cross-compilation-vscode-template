# top-most EditorConfig file
root = true

[*]
insert_final_newline = false
indent_style = space
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true
end_of_line = lf

[Makefile]
insert_final_newline = false
indent_style = tab
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true
end_of_line = lf
